<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soldesk.mapper.UploadFileMapper">
	<insert id="fileUpload" parameterType="java.util.Map">
		insert into nft_pic (file_num, saveFileName, filePath) 
		values(nft_pic_seq.nextval, #{saveFileName}, #{filePath})
	</insert>
	
	<select id="getFiles" resultType="com.soldesk.entity.UploadFileVO">
		select * from nft_pic
	</select>
</mapper>